{
  "name": "stagetimer-screenshotone-proxy",
  "main": "worker.js",
  "compatibility_date": "2025-10-26",
  "compatibility_flags": ["nodejs_compat"],

  // Environment variables
  // Secrets (set using wrangler secret put):
  //   - SCREENSHOTONE_ACCESS_KEY
  //   - SCREENSHOTONE_SECRET_KEY
  //
  // Public vars (set here or via wrangler deploy):
  "observability": {
    "enabled": true
  },

  "vars": {
    "CACHE_KEY": "v1"  // Change this to invalidate all cached screenshots
  }
}
